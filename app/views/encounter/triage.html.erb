<%- @patient = @encounter.patient -%>

<!-- Display the images associated with this Encounter -->

<% @encounter.images.each do |@image| -%>
  <%= render(:partial => "image_view") %>
<% end -%>

<!-- Give the user two choices -->

<%= form_tag :action => "triage", :id => @encounter.id %>

<div id="buttons">
	<%= submit_tag "Normal" %>
	<%= submit_tag "Send to Radiologist" %>
</div>
